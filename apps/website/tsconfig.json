{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "allowImportingTsExtensions": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@ws/background": [
        "../../packages/background/src/index.ts"
      ],
      "@ws/background/*": [
        "../../packages/background/src/*"
      ],
      "@ws/theme": [
        "../../packages/theme/src/index.ts"
      ],
      "@ws/theme/*": [
        "../../packages/theme/src/*"
      ]
    },
    "outDir": "../../.moon/cache/types/apps/website"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "references": [
    {
      "path": "../../packages/background"
    },
    {
      "path": "../../packages/theme"
    }
  ]
}
